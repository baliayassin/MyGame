function sec(iCUrrentSec){
	this.iID=iCUrrentSec;
	var secObj=document.createElement('div');
	secObj.className="div";
	secObj.style.width=100+"px";
	secObj.style.height=150+"px";
	
	var color='#' + Math.random().toString(16).substring(4);

	secObj.style.backgroundColor=color;
	
	var appendDiv=function(){
		
		for(i=0;i<13;i++){
		 var newDiv = document.createElement('div');
         
         newDiv.className = secObj.className;
         document.getElementsByTagName('main')[0].appendChild(secObj);
         
	}
		
		//secObj.addEventListener('click',star);
		
		
		
		
	} 
	
	
}
